#:kivy 1.8.0
Analysis:
    img4: screen4_img
    Label:
        size_hint_y: None
        pos_hint: {'center_x': 0.5, 'center_y': 0.95}
        height: '32dp'
        text: 'Step4 - Calculate the properties used for PCA'

    Label:
        size_hint_y: None
        pos_hint: {'center_x': 0.5, 'center_y': 0.90}
        height: '32dp'
        text: 'NOTE: Slow at the moment (~30 Seconds)'

    Image:
        id: screen4_img
        size: '750dp','750dp'
        size_hint: None, None
        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
        source: 'app_data/white.png'

    Button:
        size_hint: None, None
        size: '150dp', '48dp'
        pos_hint: {'center_x': 0.5, 'center_y': 0.1}
        text: 'Get Properties'
        on_release:
            root.get_props()
            screen4_img.reload()
    
    Button:
        id: next1.3
        size_hint: None, None
        size: '150dp', '48dp'
        text: 'Previous'
        pos_hint: {'center_x': 0.15, 'center_y': 0.1}
        on_release: app.go_prev_screen( num = 3 )

    Button:
        id: next1.3
        size_hint: None, None
        size: '150dp', '48dp'
        text: 'Next'
        pos_hint: {'center_x': 0.85, 'center_y': 0.1}
        on_release: 
            app.go_next_screen( num = 5 )