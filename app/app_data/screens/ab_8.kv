#:kivy 1.8.0
Analysis:
    img8: screen8_img

    Label:
        size_hint_y: None
        pos_hint: {'center_x': 0.5, 'center_y': 0.95}
        height: '32dp'
        text: 'Step8 - Plotting Position-Sensitive Biophysical Properties'
    
    Image:
        id: screen8_img
        size: '600dp','600dp'
        size_hint: None, None
        pos_hint: {'center_x': 0.50, 'center_y': 0.5}
        source: 'app_data/white.png'

    Button:
        size_hint: None, None
        size: '150dp', '48dp'
        pos_hint: {'center_x': 0.5, 'center_y': 0.1}
        text: 'Get Properties'
        on_release:
            root.get_pos_props()
            screen8_img.reload()

    Button:
        id: next1.3
        size_hint: None, None
        size: '150dp', '48dp'
        text: 'Previous'
        pos_hint: {'center_x': 0.15, 'center_y': 0.1}
        on_release: app.go_prev_screen( num = 7 )

    Button:
        id: next1.3
        size_hint: None, None
        size: '150dp', '48dp'
        text: 'Next'
        pos_hint: {'center_x': 0.85, 'center_y': 0.1}
        on_release: 
            #root.check_checks()
            app.go_next_screen( num = 9 )